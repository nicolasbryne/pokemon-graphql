  held_item_id            items?             @relation("itemsTopokemon_evolution_held_item_id")
  known_move_id           moves?
  location_id             locations?
  party_species_id        pokemon_species?   @relation("pokemon_evolution_party_species_idTopokemon_species")
  trade_species_id        pokemon_species?   @relation("pokemon_evolution_trade_species_idTopokemon_species")
