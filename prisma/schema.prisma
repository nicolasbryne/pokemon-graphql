model move_meta {
  ailment_chance   Int
  crit_rate        Int
  flinch_chance    Int
  healing          Int
  max_hits         Int?
  max_turns        Int?
  min_hits         Int?
  min_turns        Int?
  recoil           Int
  stat_chance      Int
  meta_ailment_id  move_meta_ailments
  meta_category_id move_meta_categories
  /// This used to be part of a relation to moves
  move_id          Int                  @default(autoincrement()) @id

  @@index([healing], name: "ix_move_meta_healing")
  @@index([stat_chance], name: "ix_move_meta_stat_chance")
  @@index([ailment_chance], name: "ix_move_meta_ailment_chance")
  @@index([crit_rate], name: "ix_move_meta_crit_rate")
  @@index([flinch_chance], name: "ix_move_meta_flinch_chance")
  @@index([max_turns], name: "ix_move_meta_max_turns")
  @@index([min_turns], name: "ix_move_meta_min_turns")
  @@index([recoil], name: "ix_move_meta_recoil")
  @@index([min_hits], name: "ix_move_meta_min_hits")
  @@index([max_hits], name: "ix_move_meta_max_hits")
}

model pokemon_evolution {
  gender                  String?
  id                      Int                @default(autoincrement()) @id
  minimum_beauty          Int?
  minimum_happiness       Int?
  minimum_level           Int?
  relative_physical_stats Int?
  time_of_day             String?
